                    "check_uuid": null,
                    "disable_autoformat": true,
                    "formula": "=round(VAL,1)",
                    "metric_display_name": "key cache hit pct",
                    "metric_name": "cassandra_info`key_cache_hit_pct",
                    "period": 0,
                    "range_high": 100,
                    "range_low": 50,
                    "thresholds": {
                        "colors": [
                            "#008000",
                            "#ffcc00",
                            "#ee0000"
